{
  "name": "provider-manager-backend",
  "version": "0.1.0",
  "private": true,
  "description": "Provider Manager IA - Backend (NestJS)",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "test": "jest",
    "migration:run": "node --require ts-node/register --require dotenv/config ./node_modules/typeorm/cli.js -d src/data-source.ts migration:run",
    "migration:generate": "node --require ts-node/register --require dotenv/config ./node_modules/typeorm/cli.js -d src/data-source.ts migration:generate src/migrations/AutoMigration",
    "seed:pricing": "ts-node -r dotenv/config src/pricing/seed.ts",
    "api-key:create": "ts-node -r dotenv/config src/auth/create-api-key.ts",
    "seed:demo": "ts-node -r dotenv/config src/seed/demo.ts",
    "seed:docs:overwrite": "ts-node -r dotenv/config src/docs/seed-overwrite.ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.637.0",
    "@aws-sdk/client-sqs": "^3.637.0",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "bullmq": "^5.12.0",
    "cache-manager": "^5.4.0",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "google-auth-library": "^9.14.0",
    "ioredis": "^5.4.1",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.9.14",
    "rate-limiter-flexible": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stripe": "^20.3.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/testing": "^10.3.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/nodemailer": "^6.4.15",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
